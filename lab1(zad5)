package com.company;
import java.util.Scanner;

public class Main {
    public static void main(String[] args) {
        Scanner num_1 = new Scanner (System.in);
        System.out.print("Введите число корень которого надо найти:");
        float n;
        n = num_1.nextFloat();
        System.out.println(Math.sqrt(n));
    }
    }

