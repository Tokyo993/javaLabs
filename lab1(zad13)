package com.company;
import java.util.Scanner;

public class Main {

    public static void main(String[] args) {
        System.out.println("таблица 3 на 5 из элементов размером до 10ти");
        int[][] arr = new int[3][5];
        for (int i=0;i<3;i++){
            for(int j=0; j<5; j++) {
                arr[i][j]=(int)(Math.random()*10);
                System.out.print(arr[i][j]+" ");
            }
            System.out.println();
        }
    }
    }

