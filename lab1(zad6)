package com.company;
import java.util.Scanner;

public class Main {
    static int calculateFactorial(int n){
        int result = 1;
        for (int i = 1; i <=n; i ++){
            result = result*i;
        }
        return result;
    }

    public static void main(String[] args){
        int i = 1;
        while(i <= 10){
            System.out.print("Факториал числа " + i + " ");
            System.out.println(calculateFactorial(i));
            i++;
        }
    }}


